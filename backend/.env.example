# PROVENIQ Properties - Environment Configuration

# Database (PostgreSQL)
DATABASE_URL=postgresql+asyncpg://proveniq:proveniq_dev@localhost:5432/proveniq_properties

# App
APP_NAME=PROVENIQ Properties
DEBUG=true
API_V1_PREFIX=/v1

# Firebase Auth
FIREBASE_PROJECT_ID=your-firebase-project-id
GOOGLE_APPLICATION_CREDENTIALS=path/to/firebase-service-account.json

# Storage Provider (gcs or s3)
STORAGE_PROVIDER=gcs

# GCS Config (if STORAGE_PROVIDER=gcs)
GCS_BUCKET_NAME=proveniq-properties-evidence
GCS_PROJECT_ID=your-gcp-project-id

# S3 Config (if STORAGE_PROVIDER=s3)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
S3_BUCKET_NAME=proveniq-properties-evidence

# Presigned URL Config
PRESIGN_TTL_SECONDS=300
MAX_UPLOAD_SIZE_MB=50

# Mason AI (advisory only)
MASON_ENABLED=true
